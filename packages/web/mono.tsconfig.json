{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "sourceMap": false,
    "outDir": "./",
    "removeComments": true,
    "declaration": true
  },
  "include": [
    "next-env.d.ts",
    "./index.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}